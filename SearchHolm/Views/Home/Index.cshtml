<script>
    function searchClick() {
        var e = document.getElementById("StateId");
        var selectedState = e.options[e.selectedIndex].value;
        console.log(selectedState)
        var ee = document.getElementById("selectCity");
        var selectedCity = ee.options[ee.selectedIndex].value;
        console.log(selectedCity)
        $.ajax({
            type: 'get',
            url: '@Url.Action("searchSubmit", "Home")',
            data: { StateId: selectedState, CityId: selectedCity },
            success: function (response) {
                $("#findPropertiesInCity").html(response)
                for (var i = 0; i < response; i++) {
                    console.log(response[i].Id)
                }
            },
            error: function (response) {
                console.log(response)
            }
        })
    }
</script>
